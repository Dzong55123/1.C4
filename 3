# 定義矩陣A
A = [[1, 2, 3],
     [4, 5, 6],
     [7, 8, 9]]

# 定義矩陣B
B = [[9, 8, 7],
     [6, 5, 4],
     [3, 2, 1]]

# 初始化矩陣C
C = [[0, 0, 0],
     [0, 0, 0],
     [0, 0, 0]]

# 遍歷矩陣A和B，將對應位置的元素相加，並將結果放入矩陣C中
for i in range(len(A)):
    for j in range(len(A[0])):
        C[i][j] = A[i][j] + B[i][j]

# 輸出矩陣C
print("矩陣C：")
for row in C:
    print(row)
